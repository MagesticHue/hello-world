// Distancia Entre Dois Pontos ...

#include <stdio.h>
#include <math.h>
typedef struct _ponto{
     float x,y;
     } ponto;

int main(){
 
  ponto p1,p2;  
    float d;
  puts("Digite As Coordenadas");
     scanf("%f" "%f" "%f" "%f",&p1.x,&p1.y,&p2.x,&p2.y);
    
    d=sqrt(pow(p2.x-p1.x,2) + pow(p2.y-p1.x.2));
       
       printf("%.2f",d);


    getch();
}
